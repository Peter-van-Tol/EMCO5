                        "dir_neg_pin": "j10:5"
                    },
                    "soft_stop": true
                }, {
                    "pins" : {
                        "stepgen_type": "step_dir_differential",
                        "step_pos_pin": "j10:4",
                        "step_neg_pin": "j10:2",
                        "dir_pos_pin": "j10:1",
                        "dir_neg_pin": "j10:0"
                    },
                    "soft_stop": true
