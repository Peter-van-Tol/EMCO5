            "module_type": "toolerator",
            "instances": [
                {
                    "tool_count": 6,
                    "ppr": 48000,
                    "over_travel": 10,
                    "stepgen": {
                        "speed": {
                            "max_acc": 6400,
                            "max_vel": 3200
                        },
                        "pins": {
                            "step_pin": "j15:6",
                            "dir_pin" : "j15:5"
                        },
                        "timings": {
                            "steplen": 1900,
                            "dir_hold_time": 650, 
                            "dir_setup_time": 650 
