    "etherbone": {
        "ip_address": "10.0.0.10",
        "mac_address": "0x10e2d5000000"
    },
    "modules": [
        {
            "module_type": "gpio",
            "instances": [ 
                {"direction": "in",  "pin":"j1:0",  "name":"home-x"},
                {"direction": "in",  "pin":"j1:1",  "name":"home-z"},
                {"direction": "in",  "pin":"j1:2"},
                {"direction": "in",  "pin":"j1:6",  "name":"reset-feed-override"},
                {"direction": "in",  "pin":"j2:2",  "name":"reset-rapid-override"},
                {"direction": "in",  "pin":"j2:6",  "name":"reset-spindle-override"},
                {"direction": "in",  "pin":"j3:0",  "name":"axis-x-selected"},
                {"direction": "in",  "pin":"j3:1",  "name":"axis-z-selected"},
                {"direction": "in",  "pin":"j3:2"},
                {"direction": "in",  "pin":"j3:4",  "name":"speed1-selected"},
                {"direction": "in",  "pin":"j3:5",  "name":"speed2-selected"},
                {"direction": "in",  "pin":"j3:6",  "name":"speed3-selected"},
                {"direction": "in",  "pin":"j4:0",  "name":"start_button"},
