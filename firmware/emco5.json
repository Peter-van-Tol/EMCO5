                {"direction": "in",  "pin":"j7:5",  "name":"alarm-z"},
                {"direction": "in",  "pin":"j7:6",  "name":"alarm-x"},
                {"direction": "out", "pin":"j15:0"},
                {"direction": "out", "pin":"j15:1"},
                {"direction": "out", "pin":"j15:2"},
                {"direction": "out", "pin":"j15:4"},
                {"direction": "out",  "pin":"j16:0", "name":"stop-LED"},
                {"direction": "out",  "pin":"j16:1", "name":"pause-LED"},
                {"direction": "out",  "pin":"j16:2", "name":"start-LED"},
                {"direction": "out",  "pin":"j16:4", "name":"spindle-override-LED"},
